{
  "name": "pokedex-js",
  "version": "0.0.1",
  "description": "A mini-encyclopedia of Pokémon species, types, evolutions, and moves.",
  "main": "index.js",
  "scripts": {
    "sass-watch": "sass --watch assets/sass/main.scss:assets/css/main.css",
    "sass-pc": "sass --watch assets/sass/pokemon-card.scss:assets/css/pokemon-card.css",
    "build": "sass assets/sass/main.scss:assets/css/main.min.css --style compressed",
    "build-pc": "sass assets/sass/pokemon-card.scss:assets/css/pokemon-card.min.css --style compressed"
  },
  "author": "Alexsander Lopes Camargos <alcamargos@vivaldi.net> (https://alcamargos.me/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexcamargos/JavaScript-Pokedex"
  },
  "homepage": "https://jspokedex-pokeapi.netlify.app",
  "license": "The MIT License (MIT)",
  "keywords": [
    "JavaScript",
    "Sass",
    "HTML",
    "Fetch API",
    "PokeAPI",
    "Pokémon",
    "Pokedex"
  ],
  "dependencies": {
    "normalize.css": "^8.0.1",
    "sass": "^1.56.0"
  }
}
